{
  "version": "1.0.0",
  "name": "maximnara/business-days-counter",
  "description": "Counts business days difference.",
  "keywords": ["maximnara", "laravel", "holidays", "difference"],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksim Luzhkov",
      "email": "maximnara@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": ">=5.5",
    "nesbot/carbon": "^1.39.0",
    "illuminate/config": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "orchestra/testbench": "3.*"
  },
  "autoload": {
    "psr-4": {
      "maximnara\\BusinessDaysCounter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "maximnara\\BusinessDaysCounter\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "maximnara\\BusinessDaysCounter\\BusinessDaysCounterServiceProvider"
      ]
    }
  }
}
